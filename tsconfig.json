{
  "compilerOptions": {
    "moduleResolution": "node16",
    "target": "ESNext",
    "module": "node16",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["functions", "src"]
}